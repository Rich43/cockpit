type Project implements PlanItemBase {
    snapshotId: ID!
    id: ID!
    ownerPersonnelId: String!
    name: String!
    description: String
    lifecycleStatus: String!
    targetPercentageComplete: Float
    actualPercentageComplete: Float
}

extend type Query {
    findAllProjects(snapshotId: ID!): [Project]!
    findProject(snapshotId: ID!, id: ID!): Project!
}