type Snapshot {
    id: ID!
    date: String!
    executionStartedAt: String
    executionEndedAt: String
    isLatestCompletedSnapshot: Boolean!
    executionStatus: String!
}

type PlanItem {
    snapshotId: ID!
    id: ID!
    ownerPersonnelId: Int!
    name: String!
    description: String
    lifecycleStatus: String!
}

type Query {
    findAllSnapshots: [Snapshot]!
    findSnapshot(id: ID!) : Snapshot!
}